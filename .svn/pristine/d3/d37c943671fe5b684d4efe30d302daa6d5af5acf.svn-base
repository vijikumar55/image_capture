     top: 0px;
    left: 0px;
    width: 1130px;
    height: 702px;
    font-family: Arial,Helvetica,sans-serif;
    font-size: 36px;
    -webkit-user-select: none;
}

#resultsheaderleft, #resultsheaderright{
    position: absolute;
    top: -1px;
    height: 200px;
    border-right-style: solid;
    border-left-style: solid;
    border-bottom-style: solid;
    border-top-style: solid;
    border-right-width: 1px;
    border-left-width: 1px;
    border-bottom-width: 1px;
    border-top-width: 1px;
    border-right-color: black;
    border-left-color: black;
    border-bottom-color: black;
    border-top-color: black;
    background-color: #ffffcc;
    -webkit-user-select: none;
}


#resultsheaderleft, #resultsleft{
    left: -1px;
    width: 564px;
}
#resultsheaderright, #resultsright{
    left: 565px;
    width: 564px;
}


#resultsheaderlefttext, #resultsheaderrighttext{
    text-align: center;
    position: absolute;
    top: 0px;
    left: 0px;
    width: 90%;
    font-weight: bold;
    padding-left: 10px;
}

#resultsleft,#resultsright{
    height: 500px;
    border-right-style: solid;
    border-left-style: solid;
    border-bottom-style: solid;
    border-top-style: solid;
    border-right-width: 1px;
    border-left-width: 1px;
    border-bottom-width: 1px;
    border-top-width: 1px;
    border-right-color: black;
    border-left-color: black;
    border-bottom-color: black;
    border-top-color: black;
    position: absolute;
}
#resultsleft, #resultsright{
    top: 200px;
}







